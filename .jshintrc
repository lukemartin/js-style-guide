{
  /* Globals */
  "globals": {},

  /* Environments */
  "browser": true,
  "jquery": true,
  "node": true,

  /* Enforcing */
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "es3": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": false,
  "nownew": true,
  "plusplus": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "maxparams": false,
  "maxdepth": false,
  "maxstatements": false,
  "maxcomplexity": false,
  "maxlen": 80,

  /* Relaxing */
  "asi": false,
  "boss": false,
  "debug": false,
  "eqnull": false,
  "esnext": false,
  "evil": false,
  "expr": false,
  "funcscope": false,
  "globalscript": false,
  "iterator": false,
  "lastsemic": false,
  "laxbreak": false,
  "laxcomma": true,
  "loopfunc": false,
  "moz": false,
  "multistr": false,
  "notypeof": false,
  "proto": false,
  "scripturl": false,
  "smarttabs": false,
  "shadow": false,
  "sub": false,
  "supernew": false,
  "validthis": false
}